<section class="content-main">
    <div class="content-header">
        <h2 class="content-title">Users list</h2>
        <div>
            <a href="#" class="btn btn-primary"><i class="material-icons md-plus"></i>Add User</a>
        </div>
    </div>
    <div class="card mb-4">
        <header class="card-header">
            <div class="row gx-3">
                <div class="col-lg-4 col-md-6 me-auto">
                    <input type="text" placeholder="Search..." class="form-control">
                </div>
                <div class="col-lg-2 col-md-3 col-6">
                    <select class="form-select">
                        <option>Status</option>
                        <option>Active</option>
                        <option>Disabled</option>
                        <option>Show all</option>
                    </select>
                </div>
                <div class="col-lg-2 col-md-3 col-6">
                    <select class="form-select">
                        <option>Show 20</option>
                        <option>Show 30</option>
                        <option>Show 40</option>
                    </select>
                </div>
            </div>
        </header> <!-- card-header end// -->
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th>User</th>
                            <th>Email</th>
                            <th>Phone</th>
                            <th>Status</th>
                           
                            <th class="text-end"> Action </th>
                        </tr>
                    </thead>
                    <tbody>
                        <% users.forEach(element => { %>
                         
                       
                        <tr>
                            <td width="40%">
                                <a href="#" class="itemside">
                            
                                    <div class="info pl-3">
                                        <h6 class="mb-0 title"><%= element.userName %> </h6>
                                    
                                    </div>
                                </a>
                            </td>
                            <td><%= element.userEmail %> </td>
                            <td><%= element.userPhone %> </td>
                            <% if (element.status) { %>
                            <td><span class="badge rounded-pill alert-success">unblocked</span>

                              <% }else{ %>
                                <td><span class="badge rounded-pill alert-danger">blocked</span>
                            <% } %> </td>

                            <td class="text-end">
                                <% if (element.status) { %>
                                  <a href="/admin/block/<%= element._id %>" class="btn btn-sm btn-danger rounded font-sm mt-15">Block</a>
                                <% }else{ %>
                                    <a href="/admin/unblock/<%= element._id %>" class="btn btn-sm btn-success rounded font-sm mt-15">Unblock</a>
                                    <% } %> 
                               
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table> <!-- table-responsive.// -->
            </div>
        </div> <!-- card-body end// -->
    </div> <!-- card end// -->
