                  <!-- MAIN SLIDES -->
                  <div class="product-image-slider" style="background-color:rgb(238, 237, 237);">
                    <% productDetails.image.forEach(element => { %>


                    <figure class="border-radius-10">
                      <img src="/images/<%= element %>" alt="product image">
                    </figure>
                    <% }) %>
                  </div>
                  <!-- THUMBNAILS -->
                  <div class="slider-nav-thumbnails pl-15 pr-15" style="height: 116px;">
                    <% productDetails.image.forEach(element => { %>
                    <div><img src="/images/<%= element %>" alt="product image"></div>
                    <% }) %>
                  </div>
                </div>
                <!-- End Gallery -->
