                    <div class="product-extra-link2">

                      <% if (locals.user) { %>
                      <button type="submit" onclick="addToCart('<%= productDetails._id%>')" id="addtoCart" class="button button-add-to-cart">Add to cart</button>
                      <a aria-label="Add To Wishlist" class="action-btn hover-up" onclick="addToCart('<%= productDetails._id%>')"><i class="fi-rs-heart"></i></a>


